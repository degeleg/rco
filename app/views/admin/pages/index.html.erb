<% heading "Dashboard" %>

<div class="row">
	<div class="col-md-3 col-sm-6">
		<div class="dash-summary bg-primary">
			<div class="content">
				<h1 class="text-left"><%= @pages.publish.size + 1 %></h1>
				<p>Pages</p>
			</div>
		</div>
	</div>

	<div class="col-md-3 col-sm-6">
		<div class="dash-summary bg-tarawera">
			<div class="content">
				<h1 class="text-left"></h1>
				<p>Subscribers</p>
			</div>
		</div>
	</div>

	<div class="col-md-3 col-sm-6">
		<div class="dash-summary bg-sea">
			<div class="content">
				<h1 class="text-left"></h1>
				<p>Administrator(s)</p>
			</div>
		</div>
	</div>

	<div class="col-md-3 col-sm-6">
		<div class="dash-summary bg-carmine">
			<div class="content">
				<h1 class="text-left"></h1>
				<p>To Do Tasks</p>
			</div>
		</div>
	</div>

</div>

<div class="row">
	<div class="col-md-6 col-sm-12 pages">
		<%= render 'admin/pages/partials/pages' %>
	</div>

	<div class="col-md-6 col-sm-12">
		<%= render 'admin/pages/partials/drafts' %>
	</div>
</div>

<div class="row">
	<%= render 'admin/pages/partials/admins' %>	
</div>

<div class="row">
	

</div>








